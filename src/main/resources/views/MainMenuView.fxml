<MenuBar prefHeight="32.0" prefWidth="266.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="storage.view.MainMenuViewController">
  <menus>
    <Menu mnemonicParsing="false" text="Rendelések">
      <items>
        <MenuItem fx:id="orderList" mnemonicParsing="false" onAction="#openOrderList" text="Rendelések listája" />
      </items>
    </Menu>
    <Menu mnemonicParsing="false" text="Termékek">
      <items>
        <MenuItem mnemonicParsing="false" onAction="#openProductList" text="Termékek listája" />
            <MenuItem mnemonicParsing="false" onAction="#openProductForm" text="Új termék rögzítése" />
      </items>
    </Menu>
    <Menu mnemonicParsing="false" text="Vásárlók">
      <items>
        <MenuItem mnemonicParsing="false" text="About" />
      </items>
    </Menu>
  </menus>
</MenuBar>
